{
  "name": "@aimey/feedback-widget",
  "version": "1.0.0",
  "description": "A React feedback widget with screenshot annotation capabilities",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vite",
    "preview": "vite preview",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "react",
    "feedback",
    "widget",
    "screenshot",
    "annotation"
  ],
  "author": "Aimey",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "tailwindcss": ">=3.0.0"
  },
  "dependencies": {
    "html-to-image": "^1.11.13",
    "konva": "^10.0.7",
    "react-konva": "^19.0.10"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^4.2.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vite": "^5.1.0"
  }
}
